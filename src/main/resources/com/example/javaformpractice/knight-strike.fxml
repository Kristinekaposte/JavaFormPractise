<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="strikePane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javaformpractice.ActionController">
   <children>
      <Label alignment="TOP_CENTER" layoutX="28.0" layoutY="34.0" prefHeight="18.0" prefWidth="520.0" text="Strike the Black Knight!" textFill="#ea0404">
         <font>
            <Font size="39.0" />
         </font>
      </Label>
      <Button fx:id="strikeButtonText" layoutX="244.0" layoutY="263.0" mnemonicParsing="false" onAction="#strikeButtonClicked" text="Strike">
         <font>
            <Font size="23.0" />
         </font>
      </Button>
      <Label fx:id="nameLabelId" layoutX="21.0" layoutY="106.0" prefHeight="38.0" prefWidth="560.0" text="Name: ">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <Button fx:id="exitButton" layoutX="262.0" layoutY="340.0" mnemonicParsing="false" onAction="#exitButtonClicked" text="Exit" />
   </children>
</AnchorPane>
